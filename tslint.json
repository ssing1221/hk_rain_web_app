{
  "rules": {
    "class-name": true,
    "curly": true,
    "eofline": false,
    "forin": true,
    "indent": [
      true,
      4
    ],
    "allowUnusedLabels": false,
    "label-position": true,
    "max-line-length": [
      true,
      200
    ],
    "no-arg": true,
    "no-bitwise": true,
    "no-console": [
      false,
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-eval": true,
    "no-string-literal": false,
    "no-trailing-whitespace": false,
    "no-unused-variable": false,
    "radix": true,
    "semicolon": [true],
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "variable-name": false
  }
}
